{
  "manifest_version": 2,

  "name": "Khrome",
  "description": "KeePass 2 plugin for Google Chrome browser",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Show options"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
